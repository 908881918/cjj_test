/* classMenu.wxss */
#classMenu{
  width: 750rpx;
  height: 100%;
  font-size: 12px;
  background-color: white;
  color: rgb(153, 153, 153);
  line-height: 3em;
}
#turnBack{
  font-size: 14px;
  font-weight: bold;
  line-height: 2.5em;
  text-align: center;
  border-top: solid 12px rgb(238, 238, 238);
  border-bottom: solid 12px rgb(238, 238, 238);
}
.classTitle{
  clear: left;
  color: black;
  margin-left: 6px;
}
.getAll{
  color: rgb(208, 208, 208);
  float: right;
  margin-right: 12px;
}
.classOdd,.classEven{
  float: left;
  background-color: rgb(248, 248, 248);
  width: 350rpx;
  padding-left: 20rpx;
  margin-bottom: 3px;
}
.classOdd{
  margin-right: 10rpx;
}